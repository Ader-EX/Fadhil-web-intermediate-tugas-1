{
  "extends": "next/core-web-vitals",
  /*
   * Buatlah minimal 5 konfigurasi ESLint untuk opsi RULES.
   * Selanjutnya, install Prettier secara lokal dan buatlah minimal 3 opsi konfigurasi.
   */
  "rules": {
    "prefer-const": "warn",
    "no-var": "error",
    "semi": "error",
    "arrow-spacing": "error",
    "no-console": "warn"
  }
}
